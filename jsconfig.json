{
  "extends": "@tsconfig/node12/tsconfig.json",
  "compilerOptions": {
    "noImplicitAny": true,
    "moduleResolution": "node",
    "lib": ["DOM", "es2019", "es2020.promise", "es2020.bigint", "es2020.string"],
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@typings/*": ["typings/*"],
      "@architect/shared/*": ["src/shared/*"]
    }
  },
  "include": ["./typings", "./src"],
  "exclude": ["node_modules"]
}
