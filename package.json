    "tape": "5.1.1",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  },
  "scripts": {
    "build:tsc": "tsc",
    "build:rollup": "rollup -c",
    "build": "run-s build:*",
    "dev": "tsc -w & rollup -c -w",
    "lint": "eslint src --fix",
    "start": "cross-env NODE_ENV=testing ARC_LOCAL=1 npx sandbox",
